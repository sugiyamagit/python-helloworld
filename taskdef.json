{
  "taskDefinition": {
    "family": "hello-python",
    "containerDefinitions": [
      {
        "name": "hello-python",
        "image": "619003409903.dkr.ecr.ap-northeast-1.amazonaws.com/hello-python:latest",
        "cpu": 100,
        "portMappings": [
          {
            "protocol": "tcp",
            "containerPort": 8080,
            "hostPort": 8080
          }
        ],
        "memory": 128,
        "essential": true
      }
    ]
  }
}
