{
    "executionRoleArn": "arn:aws:iam::619003409903:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "name": "hello-python",
        "image": "<IMAGE1_NAME>",
        "portMappings": [
          {
            "containerPort": 8080,
            "hostPort": 8080,
            "protocol": "tcp"
          }
        ],
        "essential": true
      }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "hello-python"
  }
